{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "dependencyDashboard": true,
  "dependencyDashboardApproval": false,
  "dependencyDashboardAutoclose": false,
  "bumpVersion": "minor",
  "enabledManagers": [
    "helm-values"
  ],
  "helm-values": {
    "fileMatch": [
      "(^|/charts/.*)/values\\.yaml$"
    ]
  }
}
